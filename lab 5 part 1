      "cell_type": "code",
      "source": [
        "# Cell 2: Import libraries\n",
        "import requests\n",
        "from bs4 import BeautifulSoup\n",
        "from transformers import pipeline\n",
        "import re\n",
        "from collections import Counter\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "from google.colab import files"
      ],
      "metadata": {
        "id": "WTBkSqKXMHTB"
      },
      "execution_count": 2,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Cell 3: Define scraper function\n",
        "def scrape_page(url, max_pages=5, visited=None):\n",
        "    if visited is None:\n",
        "        visited = set()\n",
        "\n",
        "    if url in visited or len(visited) >= max_pages:\n",
        "        return []\n",
        "\n",
        "    visited.add(url)\n",
        "    results = []\n",
        "\n",
        "    try:\n",
        "        response = requests.get(url, timeout=5)\n",
        "        response.raise_for_status()\n",
        "        soup = BeautifulSoup(response.text, 'html.parser')\n",
        "\n",
        "        text = ' '.join([p.get_text().strip() for p in soup.find_all('p')])\n",
        "        if not text:\n",
        "            text = soup.get_text().strip()[:1000]\n",
        "\n",
        "        text = re.sub(r'\\s+', ' ', text).strip()\n",
        "        results.append({'url': url, 'text': text})\n",
        "\n",
        "        links = [a.get('href') for a in soup.find_all('a') if a.get('href')]\n",
        "        links = [link for link in links if link.startswith('http') and not link.endswith(('.jpg', '.png', '.pdf'))]\n",
        "\n",
        "        for link in links[:3]:\n",
        "            results.extend(scrape_page(link, max_pages, visited))\n",
        "\n",
        "    except Exception as e:\n",
        "        print(f\"Error scraping {url}: {e}\")\n",
        "\n",
        "    return results"
      ],
      "metadata": {
        "id": "eC_IWRJWMZo5"
      },
      "execution_count": 4,
      "outputs": []
    },
