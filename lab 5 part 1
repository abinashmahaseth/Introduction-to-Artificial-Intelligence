    {
      "cell_type": "code",
      "source": [
        "# Cell 6: Extract keywords\n",
        "all_text = ' '.join([item['summary'] for item in scraped_data if item['summary'] != \"Summary not available\"])\n",
        "words = re.findall(r'\\b\\w+\\b', all_text.lower())\n",
        "stopwords = {'the', 'a', 'an', 'and', 'or', 'but', 'in', 'on', 'at', 'to', 'for', 'of', 'with', 'by'}\n",
        "words = [word for word in words if word not in stopwords and len(word) > 3]\n",
        "word_counts = Counter(words)\n",
        "top_keywords = word_counts.most_common(5)\n",
        "print(\"Top 5 Keywords:\", top_keywords)\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "hKszt2hmNn25",
        "outputId": "ba197da0-e0f3-4c25-bd18-fba738b25311"
      },
      "execution_count": 7,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Top 5 Keywords: [('from', 4), ('free', 4), ('wikipedia', 3), ('knowledge', 3), ('wikimedia', 3)]\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Cell 7: Search for related documents\n",
        "keyword = top_keywords[0][0]\n",
        "search_url = f\"https://en.wikipedia.org/wiki/{keyword.replace(' ', '_')}\"\n",
        "related_data = scrape_page(search_url, max_pages=3)\n",
        "\n",
        "for item in related_data:\n",
        "    text = item['text'][:1000]\n",
        "    if len(text.split()) > 50:\n",
        "        try:\n",
        "            summary = summarizer(text, min_length=30, max_length=100, do_sample=False)[0]['summary_text']\n",
        "            item['summary'] = summary\n",
        "        except Exception as e:\n",
        "            print(f\"Error summarizing {item['url']}: {e}\")\n",
        "            item['summary'] = \"Summary not available\"\n",
        "    else:\n",
        "        item['summary'] = \"Text too short to summarize\"\n",
        "\n",
        "print(f\"\\nRelated Pages for Keyword '{keyword}':\")\n",
        "for item in related_data:\n",
        "    print(f\"URL: {item['url']}\")\n",
        "    print(f\"Summary: {item['summary']}\\n\")"
      ],
      "metadata": {
